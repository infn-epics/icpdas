
record(ai, "$(P):$(R):AI$(ID)_RB")
{
    field(DESC, "$(DESC=Analog Input $(ID))")
    field(DTYP, "asynFloat64")
    field(INP,  "@asyn($(PORT) $(ID) $(TIMEOUT=1000))INT16")
    
    field(SCAN, "I/O Intr")
    field(FLNK, "$(P):$(R):AI$(ID):VOLT_RB")
}
