{
    field(DESC, "$(DESC=Max Analog Input $(ID))")
    field(DTYP, "asynFloat64")
    field(INP,  "@asyn($(PORTMAX) $(ID) $(TIMEOUT=1000))INT16")
    field(SCAN, "I/O Intr")
