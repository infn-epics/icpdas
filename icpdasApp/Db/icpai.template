record(calc, "$(P):$(R):AI$(ID):VOLT_RB")
{
    field(DESC, "$(DESC=Volt $(ID))")
    field(INPA, "$(P):$(R):AI$(ID)_RB")
    field(CALC, "A / 32768. * $(maxscale=10.)")
    field(PREC, "3")
    field(EGU, "V")
}
