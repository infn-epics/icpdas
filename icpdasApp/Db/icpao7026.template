record(mbbo, "$(P):$(R):$(ID)_OUT")
{
    
    field(DTYP, "asynInt32")
    field(OUT, "@asyn($(PORTMAX) $(REGOFFSET) 0xFFFF)")
    field(DESC, "OUT $(ID)")
    field(SCAN, "I/O Intr")
    field(ZRVL, "0x30")
    field(ZRST, "0~20 mA")
    field(ONVL, "0x31")
    field(ONST, "4~20 mA")
    field(TWVL, "0x32")
    field(TWST, "0~10 V")
    field(THVL, "0x33")
    field(THST, "+/-10 V")
    field(FRVL, "0x34")
    field(FRST, "0~5 V")
    field(FVVL, "0x35")
    field(FVST, "+/-5 V")
}
