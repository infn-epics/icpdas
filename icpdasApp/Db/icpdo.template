
record(bo, "$(P):$(R):DO$(ID)_SP")
{
    field(DESC, "$(DESC=DO$(ID) Set point)")
    field(DTYP, "asynUInt32Digital")
    field(OUT,  "@asynMask($(PORT) $(ID) 1)")
    field(ONAM, "ON")
    field(ZNAM, "OFF")
    field(SCAN, "Passive")
    field(FLNK,"$(P):$(R):DO$(ID)_RB")
}

record(bi, "$(P):$(R):DO$(ID)_RB")
{
    field(DESC, "$(DESC=DO$(ID) RB)")
    field(DTYP, "asynUInt32Digital")
    field(INP,  "@asynMask($(PORT) $(ID) 1)")
    field(ONAM, "ON")
    field(ZNAM, "OFF")
    field(SCAN, "Passive")
}

